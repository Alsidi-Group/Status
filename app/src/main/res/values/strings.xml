    <string name="preference_icon_scale">Icon Scale (dp)</string>
    <string name="preference_color_notification">Screen-Specific Color Notifications</string>
    <string name="preference_color_notification_desc">The app will show a notification as a shortcut to set the color for an app if the a color has not been found.</string>
    <string name="preference_center_gravity">Center Icon</string>
    <string name="preference_center_gravity_desc">This will move the icon to the center of the status bar.</string>

