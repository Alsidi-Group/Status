    <string name="preference_show_clock">Show Clock</string>
    <string name="preference_24h">Use 24 Hour Time Format</string>
    <string name="preference_24h_desc">The clock will show in a 24 hour time format if this is turned on.</string>
    <string name="preference_ampm">Show AM/PM in Clock</string>
    <string name="preference_ampm_desc">Choose whether to show the AM/PM indicator in the clock.</string>
    <string name="preference_date">Show Current Date</string>
    <string name="preference_date_desc">Shows the date next to the time in the clock.</string>
    <string name="preference_show_notifications">Show Notifications</string>
    <string name="preference_heads_up">Show Heads-Up Overlays</string>
    <string name="preference_heads_up_desc">Notifications will \'peek\' from the top of the screen whenever an important notification is received. If you are on Android M or higher, you may want to disable the system heads up notifications to prevent annoying things from happening. Unfortunately it is not possible to disable them on Android L.</string>
    <string name="preference_heads_up_duration">Heads-Up Duration</string>
    <string name="preference_heads_up_duration_desc">Set the amount of time for the app to show a heads-up notification for.</string>
    <string name="preference_battery_percent">Display Battery Percent</string>
    <string name="preference_battery_percent_desc">Shows the battery percentage next to the icon in the status bar.</string>
    <string name="preference_bar_color_auto">Automatic Status Bar Coloring</string>
    <string name="preference_bar_color_auto_desc">Picks the status bar color from the current application.</string>
    <string name="preference_bar_color_chooser">Default Status Bar Color</string>
    <string name="preference_app_colors">Screen-Specific Colors</string>
    <string name="preference_app_colors_desc">Set the color for the status bar to use on a specific screen. This will override any other color options provided the \'automatic status bar coloring\' option is enabled.</string>
    <string name="preference_transparent_home">Transparency on Home Screen</string>
    <string name="preference_transparent_home_desc">This option sets the status bar\'s background as the current wallpaper when the home screen is displayed. This setting will not work correctly on live wallpapers or if your home screen\'s scrolling wallpaper option is enabled.</string>
    <string name="preference_fullscreen_apps">Fullscreen Apps</string>
    <string name="preference_fullscreen_apps_desc">The status bar will be disabled in the apps set here.</string>
    <string name="preference_dark_icons">Dark Icons</string>
    <string name="preference_dark_icons_desc">Darkens the status bar icons on light backgrounds.</string>
    <string name="preference_show_battery_icon">Show Battery Icon</string>
    <string name="preference_icon_style">Icon Style</string>
    <string name="preference_show_network_icon">Show Network Icon</string>
    <string name="preference_network_icon">Network Icon Style</string>
    <string name="preference_dual_sim">Dual Sim</string>
    <string name="preference_dual_sim_desc">If your phone has more than one sim card, this option will show two signal indicators instead of one.</string>
    <string name="preference_data">Show Data Connection</string>
    <string name="preference_data_desc">Shows whether the data connection is 2G, 3G, 4G, or unknown/none (doesn\'t show anything).</string>
    <string name="preference_show_wifi_icon">Show Wifi Icon</string>
    <string name="preference_wifi_icon">Wifi Icon Style</string>
    <string name="preference_show_gps_icon">Show GPS Icon</string>
